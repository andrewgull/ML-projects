df$ever_married <- factor(if_else(df$ever_married == "Yes", 1, 0))
# ID
df$id <- NULL
# for models working properly
df$stroke <- factor(ifelse(df$stroke == 1, "yes", "no"), levels = c("no", "yes"))
df
skim_desc <- skim(df)
skim_desc %>%
yank("factor")
df <- df %>% filter(gender != "Other")
